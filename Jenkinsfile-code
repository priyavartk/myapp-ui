pipeline {

    agent any

    stages{
        stage('aws') {
            steps {
                script{
                    /*extract IP of the instance*/
                    instanceIP = sh (
                            script: "aws ec2 describe-instances --filters Name=tag:Env,Values=dev --region us-west-1 --query 'Reservations[*].Instances[*].PrivateIpAddress' --output text --region us-east-1",
                            returnStdout: true
                    ).trim()
                    }
                }
            }
        }
    }
}
